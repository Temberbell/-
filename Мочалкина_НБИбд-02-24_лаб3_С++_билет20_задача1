 #include <iostream>
using namespace std;

int main()
{
	int chislo, konec;
	
	cin >> chislo;
	
	konec = chislo%10;
	
	switch(konec)
	{
	case 0:
	cout << "Если число заканчивается на 0, то квадрат этого числа заканчивается на 0";
	break;
	
	case 1:
	cout << "Если число заканчивается на 1, то квадрат этого числа заканчивается на 1";
	break;
	
	case 2:
	cout << "Если число заканчивается на 2, то квадрат этого числа заканчивается на 4";
	break;
	
	case 3:
	cout << "Если число заканчивается на 3, то квадрат этого числа заканчивается на 9";
	break;
	
	case 4:
	cout << "Если число заканчивается на 4, то квадрат этого числа заканчивается на 6";
	
	case 5:
	cout << "Если число заканчивается на 5, то квадрат этого числа заканчивается на 5";
	break;
	
	case 6:
	cout << "Если число заканчивается на 6, то квадрат этого числа заканчивается на 6";
	break;
	
	case 7:
	cout << "Если число заканчивается на 7, то квадрат этого числа заканчивается на 9";
	break;
	
	case 8:
	cout << "Если число заканчивается на 8, то квадрат этого числа заканчивается на 4";
	break;
	
	case 9:
	cout << "Если число заканчивается на 9, то квадрат этого числа заканчивается на 1";
	break;
	
	default: 
	cout << "Неизвестная команда";
	break;
	}
	}
